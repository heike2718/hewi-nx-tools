{
  "name": "#lib-name#",
  "$schema": "#cd-up#node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "#feature-root-path#/api/src",
  "prefix": "#feature-name#",
  "projectType": "library",
  "tags": #tags#,
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "#feature-root-path#/api/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
